    "components": {
        "schemas": {
            "ToDayWaterDataResponse": {
                "type": "object",
                "properties": {
                    "_id": {
                        "type": "string",
                        "description": "Unique identifier for the water record"
                    },
                    "date": {
                        "type": "string",
                        "format": "date",
                        "description": "Date of the water record"
                    },
                    "owner": {
                        "type": "string",
                        "description": "Owner of the water record"
                    },
                    "dailyNorma": {
                        "type": "integer",
                        "description": "Daily water consumption norm in milliliters"
                    },
                    "waterTotal": {
                        "type": "integer",
                        "description": "Total water consumption in milliliters"
                    },
                    "persantRate": {
                        "type": "integer",
                        "description": "Water consumption percentage rate"
                    },
                    "waterSavings": {
                        "type": "integer",
                        "description": "Amount of water savings in milliliters"
                    },
                    "WaterNotes": {
                        "type": "array",
                        "description": "Array of water dose notes",
                        "items": {
                            "type": "object",
                            "properties": {
                                "createdDate": {
                                    "type": "string",
                                    "format": "date-time",
                                    "description": "Date and time when the water dose note was created"
                                },
                                "waterDose": {
                                    "type": "integer",
                                    "description": "Amount of water dose in milliliters"
                                },
                                "_id": {
                                    "type": "string",
                                    "description": "Unique identifier for the water dose note"
                                }
                            },
                            "required": [
                                "createdDate",
                                "waterDose",
                                "_id"
                            ]
                        }
                    },
                    "createdAt": {
                        "type": "string",
                        "format": "date-time",
                        "description": "Date and time when the water record was created"
                    },
                    "updatedAt": {
                        "type": "string",
                        "format": "date-time",
                        "description": "Date and time when the water record was last updated"
                    }
                },
                "required": [
                    "_id",
                    "date",
                    "owner",
                    "dailyNorma",
                    "waterTotal",
                    "persantRate",
                    "waterSavings",
                    "WaterNotes",
                    "createdAt",
                    "updatedAt"
                ]
            },
            "MonthWaterResponse": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "date": {
                      "type": "string",
                      "format": "date",
                      "example": "2024-04-01"
                    },
                    "dailyNorma": {
                      "type": "integer"
                
                    },
                    "persantRate": {
                      "type": "integer"
            
                    },
                    "waterSavings": {
                      "type": "integer"
                
                    }
                  }
                }
              },
            
            "RegistrationRequest": {
                "type": "object",
                "required": [
                    "email",
                    "password"
                ],
                "properties": {
                    "email": {
                        "type": "string",
                        "description": "User's email",
                        "format": "email"
                    },
                    "password": {
                        "type": "string",
                        "description": "User's password",
                        "example": "qwerty123"
                    }
                }
            },
            "RegistrationResponse": {
                "type": "object",
                "properties": {
                    "type": "object",
                    "properties": {
                        "email": {
                            "type": "string",
                            "description": "User's email",
                            "format": "email"
                        },
                        "userId": {
                            "type": "number",
                            "description": "User's id",
                            "example": "32143232436545474"
                        }
                    }
                }
            }
        },
        "securitySchemes": {
            "Bearer": {
                "type": "http",
                "scheme": "bearer",
                "bearerFormat": "JWT"
            }
        }
    }
